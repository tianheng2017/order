define(["jquery", "easy-admin"], function ($, ea) {    var init = {        table_elem: '#currentTable',        table_render_id: 'currentTableRenderId',        index_url: 'user.extrainfo/index',        add_url: 'user.extrainfo/add',        edit_url: 'user.extrainfo/edit',        delete_url: 'user.extrainfo/delete',        export_url: 'user.extrainfo/export',        modify_url: 'user.extrainfo/modify',    };    var Controller = {        index: function () {            ea.table.render({                init: init,                cols: [[                    {type: 'checkbox'},                    {field: 'uid', title: '用户ID', search: false},                    {field: 'client_name', title: '客户端名称', search: false},                    {field: 'client_version', title: '客户端版本', search: false},                    {field: 'device_name', title: '设备名称', search: false},                    {field: 'device_model', title: '设备型号', search: false},                    {field: 'os_version', title: '系统版本号', search: false},                    {field: 'device_id', title: '设备ID', search: false},                    {field: 'udid', title: '唯一设备标识码', search: false},                    {field: 'idfa', title: '苹果设备IDFA', search: false},                    {field: 'idfv', title: '苹果设备IDFV', search: false},                    {field: 'create_time', title: '添加时间', search: false},                    {field: 'extend1', title: '扩展字段1', search: false},                    {field: 'extend2', title: '扩展字段2', search: false},                    {field: 'userAuthorize.identifier_type', search: 'select', hide:true, selectList: {"1":"手机号","2":"邮箱","3":"用户名"}, title: '授权方式'},                    {field: 'userAuthorize.area_code', search: true, hide:true, title: '区号'},                    {field: 'userAuthorize.identifier', search: true, hide:true, title: '唯一标识'},                    {                        width: 120,                        title: '操作',                        templet: ea.table.tool,                        operat: [                            [{                                text: '编辑',                                url: init.edit_url,                                method: 'open',                                auth: 'edit',                                class: 'layui-btn layui-btn-xs layui-btn-success',                                field: 'uid'                            }, {                                text: '删除',                                title: '确定删除？',                                url: init.delete_url,                                method: 'request',                                auth: 'delete',                                class: 'layui-btn layui-btn-xs layui-btn-danger',                                field: 'uid'                            }]                        ]                    }                ]],            });            ea.listen();        },        add: function () {            ea.listen();        },        edit: function () {            ea.listen();        },    };    return Controller;});