<div class="layuimini-container">
    <form id="app-form" class="layui-form layuimini-form">
        
        <div class="layui-form-item">
            <label class="layui-form-label">uid</label>
            <div class="layui-input-block">
                <input type="text" name="uid" class="layui-input" lay-verify="required" placeholder="请输入uid" value="">
            </div>
        </div>
        <div class="layui-form-item">
            <label class="layui-form-label">项目名称</label>
            <div class="layui-input-block">
                <input type="text" name="projectname" class="layui-input" placeholder="请输入项目名称" value="">
            </div>
        </div>
        <div class="layui-form-item">
            <label class="layui-form-label">担任角色</label>
            <div class="layui-input-block">
                <input type="text" name="assumerole" class="layui-input" placeholder="请输入担任角色" value="">
            </div>
        </div>
        <div class="layui-form-item">
            <label class="layui-form-label">项目开始时间</label>
            <div class="layui-input-block">
                <input type="text" name="pjtimeleft" data-date="" data-date-type="datetime" class="layui-input" placeholder="请输入项目开始时间" value="">
            </div>
        </div>
        <div class="layui-form-item">
            <label class="layui-form-label">项目结束时间</label>
            <div class="layui-input-block">
                <input type="text" name="pjtimeright" data-date="" data-date-type="datetime" class="layui-input" placeholder="请输入项目结束时间" value="">
            </div>
        </div>
        <div class="layui-form-item layui-form-text">
            <label class="layui-form-label">项目描述</label>
            <div class="layui-input-block">
                <textarea name="description" class="layui-textarea" placeholder="请输入项目描述"></textarea>
            </div>
        </div>
        <div class="layui-form-item layui-form-text">
            <label class="layui-form-label">项目业绩</label>
            <div class="layui-input-block">
                <textarea name="achievement" class="layui-textarea" placeholder="请输入项目业绩"></textarea>
            </div>
        </div>
        <div class="layui-form-item">
            <label class="layui-form-label">项目链接</label>
            <div class="layui-input-block">
                <input type="text" name="projectlink" class="layui-input" placeholder="请输入项目链接" value="">
            </div>
        </div>
        <div class="hr-line"></div>
        <div class="layui-form-item text-center">
            <button type="submit" class="layui-btn layui-btn-normal layui-btn-sm" lay-submit>确认</button>
            <button type="reset" class="layui-btn layui-btn-primary layui-btn-sm">重置</button>
        </div>

    </form>
</div>